&HEAD CHID='spruce_N2_50_cat', TITLE='(Concatenated) Spruce cone calorimeter N2.' /

&MESH IJK=3,3,3, XB=-0.15,0.15,-0.15,0.15,0,0.3 /
&MESH IJK=3,3,3, XB=-0.15,0.15,-0.15,0.15,0.3,0.6 /

&TIME T_END=1750., DT = 0.5, WALL_INCREMENT = 1 /

&MISC TMPA=27,
      Y_O2_INFTY=0.00001,
      SOLID_PHASE_ONLY = .TRUE. /

&SURF ID = 'SAMPLE',
      EXTERNAL_FLUX = 50
      THICKNESS(1:2)	    = 0.02,0.013,
      STRETCH_FACTOR(1:2)   = 1.2,2.0
      CELL_SIZE_FACTOR(1:2) = 1.0,1.0
      HEAT_TRANSFER_COEFFICIENT = 15.,
      MINIMUM_LAYER_THICKNESS = 1E-10
      BACKING='EXPOSED',
      MATL_ID(1,:)='MOISTURE','SPRUCE'
      MATL_ID(2,1)='CERAMIC',
      MATL_MASS_FRACTION(1,:) = 0.09, 0.91 /


# Start of file output.txt :

&DUMP DT_DEVC=10. /
&DUMP DT_HRR=10. /
&DUMP WRITE_XYZ=.TRUE./

&DEVC ID='HRRPUA', QUANTITY='HRRPUV', XB=-0.15,0.15,-0.15,0.15,0,0.3, SPATIAL_STATISTIC='VOLUME INTEGRAL', CONVERSION_FACTOR=100., UNITS='kW/m2' /
&DEVC ID='MLRPUA', QUANTITY='BURNING RATE', XB=-0.15,0.15,-0.15,0.15,0,0.3, SPATIAL_STATISTIC='SURFACE INTEGRAL', SURF_ID='SAMPLE', CONVERSION_FACTOR=100., UNITS='kg/m2/s' /

# Start of file spruce.txt :

&REAC FUEL='PYROLYZATE', C=6, H=10, O=5, SOOT_YIELD=0.01, HEAT_OF_COMBUSTION = 13750., RADIATIVE_FRACTION=0 /

&MATL ID                    = 'SPRUCE'
      EMISSIVITY            = 0.9
      DENSITY               = 408.
      CONDUCTIVITY_RAMP     = 'wood_k'
      SPECIFIC_HEAT_RAMP    = 'wood_cp'
      HEAT_OF_REACTION(1)   = 112.
      A(1)                  = 2.12E11
      E(1)                  = 1.91E5
      N_S(1)                = 1.89
      NU_SPEC(1,1)          = 0.85
      SPEC_ID(1,1)          = 'PYROLYZATE'
      NU_MATL(1,1)          = 0.15
      MATL_ID(1,1)          = 'CHAR' /

&MATL ID                    = 'CHAR'
      EMISSIVITY            = 0.84
      DENSITY               = 52.5
      CONDUCTIVITY_RAMP     = 'char_k'
      SPECIFIC_HEAT_RAMP    = 'char_cp'
