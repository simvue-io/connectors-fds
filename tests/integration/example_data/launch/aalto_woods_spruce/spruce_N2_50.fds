      MINIMUM_LAYER_THICKNESS = 1E-10
      BACKING='EXPOSED',
      MATL_ID(1,:)='MOISTURE','SPRUCE'
      MATL_ID(2,1)='CERAMIC',
      MATL_MASS_FRACTION(1,:) = 0.09, 0.91 /

&CATF OTHER_FILES = 'output.txt','spruce.txt','common.txt' /

&TAIL/