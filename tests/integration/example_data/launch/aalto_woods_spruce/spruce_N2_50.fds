&TIME T_END=1750., DT = 0.5, WALL_INCREMENT = 1 /

&MISC TMPA=27,
      Y_O2_INFTY=0.00001,
      SOLID_PHASE_ONLY = .TRUE. /

&SURF ID = 'SAMPLE',
      EXTERNAL_FLUX = 50
      THICKNESS(1:2)	    = 0.02,0.013,
      STRETCH_FACTOR(1:2)   = 1.2,2.0
      CELL_SIZE_FACTOR(1:2) = 1.0,1.0
